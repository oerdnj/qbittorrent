/*
-----BEGIN QCMOD-----
name: Qt >= 4.4
-----END QCMOD-----
*/
class qc_qt4 : public ConfObj
{
public:
	qc_qt4(Conf *c) : ConfObj(c) {}
	QString name() const { return "Qt >= 4.4"; }
	QString shortname() const { return "Qt 4.4"; }
	bool exec()
	{
		if(QT_VERSION >= 0x040500) {
                        conf->addDefine("QT_4_5");
                }
		return(QT_VERSION >= 0x040400);
		
	}
};
