/*
-----BEGIN QCMOD-----
name: Qt >= 4.3
-----END QCMOD-----
*/
class qc_qt4 : public ConfObj
{
public:
	qc_qt4(Conf *c) : ConfObj(c) {}
	QString name() const { return "Qt >= 4.3"; }
	QString shortname() const { return "Qt 4.3"; }
	bool exec()
	{
		if(QT_VERSION >= 0x040400) {
			conf->addDefine("QT_4_4");
		}
		return(QT_VERSION >= 0x040300);
		
	}
};
